\f1 \cf4 	var geo = new SizmekGeo();\
	Sizmek.listen(geo, [SizmekGeo.WEATHER_LOAD_COMPLETE, SizmekGeo.WEATHER_LOAD_FAILURE], weatherLoadHandler);\
	geo.loadWeatherData('0x0x0xxx0x0x0', '123456789');//These are dummy parameter values.\
	\
	function weatherLoadHandler(e)\{\
		switch(e.type)\{\
			case SizmekGeo.WEATHER_LOAD_COMPLETE:\
				console.log(e.data)//Log the successful weather data!\
				break;\
			case SizmekGeo.WEATHER_LOAD_FAILURE:\
				console.log(e.data)//Log the reason for failure!\
				break;\
		\}\
	\}\
