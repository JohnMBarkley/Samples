\pard\pardeftab720\ri-9008\sa100\partightenfactor0

\f1 \cf4 	var geo = new SizmekGeo();\
	geo.setZipCodeInputElements('zipCodeField', 'zipCodeSubmit');\
	Sizmek.listen(geo, [SizmekGeo.VALID_ZIP_SUBMIT, SizmekGeo.INVALID_ZIP_SUBMIT], zipCodeSubmitHandler);\
\
	function zipCodeSubmitHandler(e)\{\
		console.log(e.data)\
		switch(e.type)\{\
			case SizmekGeo.VALID_ZIP_SUBMIT:\
				// Initiate a new mapping search with the ZIP code from the input\
				geo.searchLocationServiceAndMap(197, 1000, 'locations', 'map', \{zip: e.data\})\
				break;\
			case SizmekGeo.INVALID_ZIP_SUBMIT:\
				// Show message that ZIP code is invalid.\
		\}\
	\}\
